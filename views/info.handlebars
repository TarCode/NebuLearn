<div class = "container2">
	{{#each info}}
		<div class="row">
			<div class="col-xs-12 col-sm-12">
		        <h3>{{this.name}}</h3>
		    </div>
		    <div class="col-xs-12 col-sm-6">
		        <img class = "img-responsive" src = "{{this.id}}">
		    </div>
		    <div class="col-xs-12 col-sm-6">
		    	<p id = 'infoPane'>{{this.info}}</p>
		    </div>
		</div>
		<hr/>
	{{/each}}
	
	<a href = "/quiz"><button type='submit' class = 'Submit'><p class = 'Submit'><br>Take Quiz</p></button></a>

</div>		
